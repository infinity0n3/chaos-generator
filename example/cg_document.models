{	
	"models": [
		{
			"name": "Document",
			"extra_deps": ["jsondocument", "file"],
			"properties":[{
				"name": "undostack",
				"type": "undostack*",
				"tags": ["no_write"]
			},{
				"name": "file_name",
				"type": "string",
				"tags": ["protected"]
			}],
			"methods":[{
				"name": "saveToFile",
				"return": "bool",
				"template": "document_save_to_json.impl.template",
				"arguments": [{
					"name": "_filename",
					"type": "string&",
					"tags": ["const"]
				}],
				"tags": ["abstract"]
			},{
				"name": "loadFromFile",
				"return": "bool",
				"template": "document_load_from_json.impl.template",
				"arguments": [{
					"name": "_filename",
					"type": "string&",
					"tags": ["const"]
				}],
				"tags": ["abstract"]
			}]
		}
	]
}
